{
  "name": "quiz-tester-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate:options": "ts-node scripts/migrate-options-format.ts",
    "migrate:options:example": "ts-node scripts/migrate-options-format.ts --example",
    "fix:numbers": "ts-node scripts/fix_question_numbers.ts",
    "fix:correct-answers": "ts-node scripts/fix-correct-answers.ts",
    "fix:correct-answers-topic": "ts-node scripts/fix-correct-answers-topic.ts",
    "fix:correct-answers-from-number": "ts-node scripts/fix-correct-answers-from-number.ts",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop quiz-tester-backend",
    "pm2:restart": "pm2 restart quiz-tester-backend",
    "pm2:delete": "pm2 delete quiz-tester-backend",
    "pm2:logs": "pm2 logs quiz-tester-backend",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:reload": "pm2 reload quiz-tester-backend",
    "pm2:setup": "chmod +x scripts/setup-pm2.sh && ./scripts/setup-pm2.sh",
    "pm2:setup:server": "chmod +x scripts/setup-server.sh && ./scripts/setup-server.sh"
  },
